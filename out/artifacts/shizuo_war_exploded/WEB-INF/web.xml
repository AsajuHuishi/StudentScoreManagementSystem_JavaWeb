<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>RegistServlet</servlet-name>
        <servlet-class>indi.huishi.web.RegistServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegistServlet</servlet-name>
        <url-pattern>/registSevlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>indi.huishi.web.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/loginServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>StudentScoreAddServlet</servlet-name>
        <servlet-class>indi.huishi.web.StudentScoreAddServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StudentScoreAddServlet</servlet-name>
        <url-pattern>/studentScoreAddServlet</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>StudentScoreDeleteServlet</servlet-name>
        <servlet-class>indi.huishi.web.StudentScoreDeleteServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StudentScoreDeleteServlet</servlet-name>
        <url-pattern>/studentScoreDeleteServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>QueryServlet</servlet-name>
        <servlet-class>indi.huishi.web.QueryServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>QueryServlet</servlet-name>
        <url-pattern>/queryServlet</url-pattern>
    </servlet-mapping>

</web-app>